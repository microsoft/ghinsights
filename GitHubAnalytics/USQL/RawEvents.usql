


@rows =
    EXTRACT Line string
    FROM "wasb://raw@kelewis.blob.core.windows.net/MongoDump/users/V1/{date:yyyy}/{date:MM}/{*}"
    USING Extractors.Tsv(Encoding.UTF8);

@count =
    SELECT COUNT(*) AS RowCount FROM @rows;

OUTPUT @count TO "/my/rowcount.txt" USING Outputters.Csv();
    