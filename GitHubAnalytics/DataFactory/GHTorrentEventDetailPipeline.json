{
  "$schema": "http://datafactories.schema.management.azure.com/schemas/2015-08-01/Microsoft.DataFactory.Pipeline.json",
  "name": "GHTorrentEventDetailPipeline",
  "properties": {
    "description": "Custom DotNetActivity pipeline to gather mongodb backups and unpack them into a new container.",
    "activities": [
      {
        "name": "MongoDbDumpTransformActivity",
        "type": "DotNetActivity",
        "inputs": [ { "name": "MongoDbDump" } ],
        "outputs": [ { "name": "EventDetailRawFiles" } ],
        "typeProperties": {
          "assemblyName": "MongoDbDumpTransformActivity.dll",
          "entryPoint": "MongoDbDumpTransformActivity.MongoDbDumpTransformActivity",
          "packageLinkedService": "GitHubAnalyticsAzureStorage",
          "packageFile": "datafactory/MongoDbDumpTransformActivity.zip",
          "extendedProperties": {
            "Year": "$$Text.Format('{0:yyyy}',SliceStart)",
            "Month": "$$Text.Format('{0:MM}',SliceStart)",
            "Day": "$$Text.Format('{0:dd}',SliceStart)"
          }
        },
        "linkedServiceName": "BatchProcessor",
        "policy": {
          "concurrency": 1,
          "executionPriorityOrder": "OldestFirst",
          "retry": 0,
          "timeout": "04:00:00"
        },
        "scheduler": {
          "frequency": "Day",
          "interval": 1
        }
      }
    ],
    "pipelineMode": "Scheduled",
    "isPaused": false,
    "start": "2015-12-01T00:00:00Z",
    "end": "2015-12-11T00:00:00Z"
  }
}
